    "autoprefixer": "^10.4.16",
    "eslint": "^8.15.0",
    "eslint-config-next": "^14.0.1",
    "eslint-config-prettier": "^9.0.0",
    "postcss": "^8.4.31",
    "prettier": "2.8.7",
    "prettier-plugin-organize-imports": "^3.2.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.4"
  }
