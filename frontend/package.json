{
  "name": "aminals-frontend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:all": "NODE_ENV=production npm run graphclient:build && next build",
    "build:cf": "NODE_ENV=production npm run graphclient:build && SKIP_DEPENDENCY_INSTALL=true npx @cloudflare/next-on-pages",
    "start": "next start",
    "start:cf": "npx wrangler pages dev .vercel/output/static",
    "lint": "next lint",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "typecheck": "tsc --pretty --noEmit",
    "graphclient:build": "graphclient build",
    "wagmi:generate": "wagmi generate"
  },
  "dependencies": {
    "@graphprotocol/client-cli": "^3.0.0",
    "@radix-ui/react-icons": "^1.3.0",
